{
    "name": "folklore/laravel-folklore",
    "description": "",
    "keywords": [
        "laravel"
    ],
    "homepage": "http://github.com/Folkloreatelier",
    "license": "MIT",
    "authors": [
        {
            "name": "Folklore",
            "email": "info@folklore.email",
            "homepage": "http://folkloreinc.ca"
        },
        {
            "name": "David Mongeau-Petitpas",
            "email": "dmp@folklore.email",
            "homepage": "http://mongo.ca",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.0.2",
        "illuminate/framework": "^8.0|^9.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "orchestra/database": "5.*|6.*|7.*",
        "orchestra/testbench": "5.*|6.*|7.*",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.5.8|^9.3.3|^9.5.10",
        "php-coveralls/php-coveralls": "^2.4"
    },
    "autoload": {
        "psr-0": {
            "Folklore": "src/"
        },
        "files": [
            "src/Folklore/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Folklore\\LocalServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
