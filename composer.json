{
    "name": "folklore/laravel-folklore",
    "description": "",
    "keywords": [
        "laravel"
    ],
    "homepage": "http://github.com/Folkloreatelier",
    "license": "MIT",
    "authors": [
        {
            "name": "Folklore",
            "email": "info@folklore.email",
            "homepage": "http://folkloreinc.ca"
        },
        {
            "name": "David Mongeau-Petitpas",
            "email": "dmp@folklore.email",
            "homepage": "http://mongo.ca",
            "role": "Developer"
        },
        {
            "name": "Nicolas Roy-Bourdages",
            "email": "nrb@folklore.email",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.0.2",
        "laravel/framework": "^8.0|^9.0|^10.0",
        "folklore/laravel-panneau": "v1.2.x-dev|v1.3.x-dev",
        "folklore/laravel-mediatheque": "v1.1.x-dev",
        "folklore/laravel-image": "v1.x-dev",
        "folklore/laravel-locale": "^8.0",
        "cviebrock/eloquent-sluggable": "^9.0|^10.0",
        "laravel/fortify": "^1.13"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "orchestra/testbench": "5.*|6.*|7.*|8.*",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.5.8|^9.3.3|^9.5.10|^10.0",
        "php-coveralls/php-coveralls": "^2.4"
    },
    "autoload": {
        "psr-0": {
            "Folklore": "src/"
        },
        "files": [
            "src/Folklore/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Folklore\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Folklore\\ServiceProvider"
            ]
        }
    },
    "suggest": {
        "pubnub/pubnub": "Allows broadcasting events via pubnub."
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
